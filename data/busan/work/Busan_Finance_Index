        "# 데이터 프레임 생성\n",
        "busan_df_index = pd.DataFrame(data)\n",
        "busan_df_index\n",
        "\n",
        "#예산관련지수들 정리\n",
        "busan_df_index['CultureBudget']=busan_CultureBudget[2021]/busan_CultureBudget['avg']*100\n",
        "busan_df_index['Cul_Goverment']=busan_BusanCul_Goverment[2021]/busan_BusanCul_Goverment['avg']*100\n",
        "busan_df_index['CulArtBudget']=busan_CulArtBudget[2021]/busan_CulArtBudget['avg']*100\n",
        "busan_df_index['Cul_Art']=busan_Cul_Art[2021]/busan_Cul_Art['avg']*100\n",
        "\n",
        "#기부지수\n",
        "busan_df_index['donation_index']=busan_BusanDonation[2021]/busan_BusanDonation['avg']*100\n",
        "\n",
        "#예산지수\n",
        "busan_df_index['budget_index'] = (\n",
        "    busan_df_index['CultureBudget']+busan_df_index['Cul_Goverment']+busan_df_index['CulArtBudget']+busan_df_index['Cul_Art']\n",
        ")/4\n",
        "\n",
        "\n",
        "#재정지수(예산+기부)\n",
        "busan_df_index['finanace_index']=(busan_df_index['donation_index']+busan_df_index['budget_index'])/2\n",
        "busan_df_index\n",
        "\n",
        "busan_df_index.to_csv('Art_FinanceIndex.csv',encoding='utf-8-sig', index=False)\n",
        "busan_df_index"
